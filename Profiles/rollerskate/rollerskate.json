{
  "name": "rollerskate",
  "description": "Tries to find a suitable way to rollerskate somewhere. Uses mostly smoothness and surface to determine where to go",
  "access": "$rollerskate.can_access",
  "oneway": "both",
  "speed": {
    "$multiply": [
      "#defaultSpeed",
      "$rollerskate.comfort"
    ]
  },
  "priority": {
    "#speedFactor": "$speed"
  },
  "behaviours": {
    "fastest": {
      "description": "The fastest (and thus smoothest) route",
      "#speedFactor": 1
    }
  },
  "defaults": {
    "#defaultSpeed": 20,
    "#speedFactor": 0
  },
  "vehicletypes": [
    "vehicle",
    "rollerskate"
  ],
  "metadata": [
    "name"
  ]
}